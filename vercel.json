{
    "rewrites": [
        { "source": "/api/(.*)", "destination": "/api/$1" },
        { "source": "/(.*)", "destination": "/index.html" },
        { "source": "/v_",
                    "has": [
                        {
                            "type": "query",
                            "key": "id",
                            "value": "(.*)"
                        }
                    ],
                    "destination": "/v_?id=:id"
                }
        
    ],
    "routes": [
        { "src": "/v_(.*)", "dest": "/v_?id=$1" } 
    ]
}
