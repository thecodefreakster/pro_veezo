{
    "rewrites": [
        { "source": "/api/(.*)", "destination": "/api/$1" },
        { "source": "/(.*)", "destination": "/index.html" },
        {
            "rewrites": [
                {
                    "source": "/v_",
                    "has": [
                        {
                            "type": "query",
                            "key": "id",
                            "value": "(.*)"
                        }
                    ],
                    "destination": "/video?id=:id"
                }
            ]
        }
        
    ],
    "routes": [
        { "src": "/v_(.*)", "dest": "/video?id=$1" } 
    ]
}
